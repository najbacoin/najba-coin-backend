{
  "name": "duel-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "start": "nodemon --ext ts --watch src --watch types --exec 'npm run start:local'",
    "start:local": "npm run node -- src/server.ts",
    "start:services": "docker-compose rm --force && docker-compose up --build",
    "test": "run-s test:types test:logic test:lint",
    "test:lint": "eslint '**/*.ts'",
    "test:lint:fix": "eslint '**/*.ts' --fix",
    "test:types": "tsc --noEmit -p .",
    "test:logic": "npm run mocha -- --timeout 2000",
    "node": "TS_NODE_TRANSPILE_ONLY=true NODE_OPTIONS=${npm_package_config_node_options} node"
  },
  "author": "",
  "license": "ISC",
  "config": {
    "node_options": "--require ts-node/register --require tsconfig-paths/register --require dotenv/config"
  },
